{
  "name": "template-react",
  "private": true,
  "version": "2.4.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:domain": "vite --host local.integrations.x-bees.com --port=443",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "version:patch": "yarn version --no-git-tag-version  --preid beta --patch",
    "version:minor": "yarn version --no-git-tag-version  --preid beta --minor",
    "version:prepatch": "yarn version --no-git-tag-version  --preid beta --prepatch",
    "version:preminor": "yarn version --no-git-tag-version  --preid beta --preminor",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix --quiet",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.5",
    "@wildix/xbees-connect": "^1.1.8",
    "@wildix/xbees-connect-react": "^1.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@wildix/eslint-config-style-guide": "^1.1.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-mkcert": "^1.17.3",
    "vitest": "^1.2.1"
  }
}
