{
  "name": "template-react",
  "private": true,
  "version": "2.4.9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:domain": "vite --host local.integrations.x-bees.com --port=443",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\" && tsc --noEmit",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix --quiet",
    "version:patch": "yarn version --no-git-tag-version  --preid beta --patch",
    "version:minor": "yarn version --no-git-tag-version  --preid beta --minor",
    "version:prepatch": "yarn version --no-git-tag-version  --preid beta --prepatch",
    "version:preminor": "yarn version --no-git-tag-version  --preid beta --preminor",
    "version:pre": "yarn version --no-git-tag-version  --preid alpha --prerelease"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.15",
    "@wildix/xbees-connect": "^1.2.9",
    "@wildix/xbees-connect-react": "^1.2.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@wildix/eslint-config-style-guide": "^1.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-mkcert": "^1.17.5",
    "vitest": "^1.5.3"
  }
}
